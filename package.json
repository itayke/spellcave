{
  "name": "spellcave",
  "description": "Type your way down an infinite cave.",
  "version": "0.1",
  "type": "module",
  "repository": {
      "type": "git",
      "url": "git+https://github.com/itayke/spellcave.git"
  },
  "author": "Itay Keren <itay@untame.com> (https://untame.com/itay)",
  "homepage": "https://untame.com/",
  "keywords": [
      "spell",
      "cave",
      "word",
      "game",
      "phaser",
      "phaser3",
      "react",
      "vite"
  ],
  "scripts": {
      "start": "npm run dev",
      "dev": "node log.js dev & vite --config vite/config.dev.mjs",
      "build": "node log.js build & vite build --config vite/config.prod.mjs",
      "dev-nolog": "vite --config vite/config.dev.mjs",
      "build-nolog": "vite build --config vite/config.prod.mjs",
      "exportTree": "node src/game/offlineCommand.js exportTree",
      "testTree": "node src/game/offlineCommand.js testTree"
  },
  "dependencies": {
      "phaser": "^3.87.0",
      "react": "^18.3.1",
      "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "terser": "^5.28.1",
    "vite": "^5.3.1"
  }
}